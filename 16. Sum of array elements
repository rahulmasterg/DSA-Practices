/*

#output

Enter the size of array = 
3
Enter the elements of array : 
5
2
1
Sum of the elements of an array = 8

*/

#include<iostream>
using namespace std;
int main(){
    int n;
    
    cout<<"Enter the size of array = "<<endl;
    cin>>n;
    
    int arr[n];
    cout<<"Enter the elements of array : "<<endl;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int sum=0;
    for(int i=0;i<n;i++){
        sum = sum + arr[i];
    }
    cout<<"Sum of the elements of an array = "<<sum;
    return 0;
}
