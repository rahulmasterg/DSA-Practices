#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    int n, flag=0;
    cout<<"Enter a number to check prime or not"<<endl;
    cin>>n;
    
    for(int i=2; i<n/2; i++){
        if(n%i==0){
            cout<<"number is not prime";
            flag=1;
            break;
        }
    }
    
    if(flag==0){
        cout<<"Number is prime";
    }

    return 0;
}
