// C++ practice programs
/*


0 1 1 2 3 5 8 13 21 ......


*/


#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    int a=0,b=1,n;
    cout<<"Enter the value of n"<<endl;
    cin>>n;
    cout<<a<<" "<<b<<" ";
    for(int i=1;i<=n;i++){
        int k=a+b;
        cout<<k<<" ";
        a=b;
        b=k;
    }

    return 0;
}
