/*


Accepted

Runtime: 0 ms

Your input
123

Output
321

Expected
321


*/

class Solution {
public:
    int reverse(int x) {
        
        int num=0;
        
        while(x!=0){
            int rem=x%10;
            //cout<<rem;
            num = num*10+rem;
            x=x/10;
        }
        
        return num;
        
    }
};
