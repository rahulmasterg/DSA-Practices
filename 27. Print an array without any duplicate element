#include <bits/stdc++.h>
using namespace std;

int main(){
	vector <int> v = {5, 6, 3, 5, 2, 3, 7};
	int n = v.size();
	
	sort(v.begin(), v.end());
	
	for(int i=0; i<n; i++){
		cout<<v[i]<<" ";
	}
	cout<<endl;
	for(int i=0; i<n; i++){
		if(v[i]!=v[i+1]){
			cout<<v[i]<<" ";
		}else{
			continue;
		}
	}
}
